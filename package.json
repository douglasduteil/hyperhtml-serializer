{
  "name": "hyperhtml-serializer",
  "version": "2.1.0",
  "description": "Jest snapshot serializer that beautifies hyperHTML snapshots.",
  "keywords": [
    "jest",
    "serializer",
    "hyperHTML",
    "testing",
    "snapshot"
  ],
  "homepage": "https://github.com/douglasduteil/hyperhtml-serializer#readme",
  "bugs": {
    "url": "https://github.com/douglasduteil/hyperhtml-serializer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglasduteil/hyperhtml-serializer.git"
  },
  "license": "WTFPL",
  "author": "Douglas Duteil <douglasduteil@gmail.com> (https://douglasduteil.github.io)",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest --verbose",
    "version": "echo \"Use the release script !\"; exit 1",
    "watch": "jest --watch"
  },
  "jest": {
    "snapshotSerializers": [
      "./index.js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "/__tests__/.*(test|spec)\\.js$"
  },
  "devDependencies": {
    "hyperhtml": "^2.34.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "peerDependencies": {
    "hyperhtml": ">=2.19"
  },
  "packageManager": "yarn@4.5.1+sha512.341db9396b6e289fecc30cd7ab3af65060e05ebff4b3b47547b278b9e67b08f485ecd8c79006b405446262142c7a38154445ef7f17c1d5d1de7d90bf9ce7054d"
}
