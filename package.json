{
  "name": "hyperhtml-serializer",
  "version": "2.1.0",
  "description": "Jest snapshot serializer that beautifies hyperHTML snapshots.",
  "keywords": [
    "jest",
    "serializer",
    "hyperHTML",
    "testing",
    "snapshot"
  ],
  "homepage": "https://github.com/douglasduteil/hyperhtml-serializer#readme",
  "bugs": {
    "url": "https://github.com/douglasduteil/hyperhtml-serializer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglasduteil/hyperhtml-serializer.git"
  },
  "license": "WTFPL",
  "author": "Douglas Duteil <douglasduteil@gmail.com> (https://douglasduteil.github.io)",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest --verbose",
    "version": "echo \"Use the release script !\"; exit 1",
    "watch": "jest --watch"
  },
  "jest": {
    "snapshotSerializers": [
      "./index.js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "/__tests__/.*(test|spec)\\.js$"
  },
  "devDependencies": {
    "hyperhtml": "^2.34.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0"
  },
  "peerDependencies": {
    "hyperhtml": ">=2.19"
  },
  "packageManager": "yarn@4.12.0+sha512.f45ab632439a67f8bc759bf32ead036a1f413287b9042726b7cc4818b7b49e14e9423ba49b18f9e06ea4941c1ad062385b1d8760a8d5091a1a31e5f6219afca8"
}
