{
  "name": "hyperhtml-serializer",
  "version": "2.0.1",
  "description": "Jest snapshot serializer that beautifies hyperHTML snapshots.",
  "keywords": [
    "jest",
    "serializer",
    "hyperHTML",
    "testing",
    "snapshot"
  ],
  "homepage": "https://github.com/douglasduteil/hyperhtml-serializer#readme",
  "bugs": {
    "url": "https://github.com/douglasduteil/hyperhtml-serializer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglasduteil/hyperhtml-serializer.git"
  },
  "license": "WTFPL",
  "author": "Douglas Duteil <douglasduteil@gmail.com> (https://douglasduteil.github.io)",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "cover": "jest --coverage",
    "test": "jest --verbose",
    "version": "echo \"Use the release script !\"; exit 1",
    "watch": "jest --watch"
  },
  "jest": {
    "snapshotSerializers": [
      "./index.js"
    ],
    "testEnvironment": "jsdom",
    "testRegex": "/__tests__/.*(test|spec)\\.js$"
  },
  "devDependencies": {
    "hyperhtml": "^2.34.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "peerDependencies": {
    "hyperhtml": ">=2.19"
  },
  "packageManager": "yarn@4.3.1+sha512.af78262d7d125afbfeed740602ace8c5e4405cd7f4735c08feb327286b2fdb2390fbca01589bfd1f50b1240548b74806767f5a063c94b67e431aabd0d86f7774"
}
